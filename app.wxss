/* pages/header/header.wxss */
view,scroll-view,swiper,movable-view,cover-view,icon,text,rich-text,progress,:checkbox,picker,picker-view,radio,slider,switch,areatext,navigator, a, abbr, acronym, address, applet, article, aside, audio, b, big, blockquote, body, canvas, caption, center, cite, code, dd, del, details, dfn, div, dl, dt, em, embed, fieldset, figcaption, figure, footer, form, h1, h2, h3, h4, h5, h6, header, hgroup, html, i, iframe, img, ins, kbd, label, legend, li, mark, menu, nav, object, ol, output, p, pre, q, ruby, s, samp, section, small, span, strike, strong, sub, summary, sup, table, tbody, td, tfoot, th, thead, time, tr, tt, u, ul, var, video{
  font: inherit;font-size: 28rpx;margin: 0;padding: 0;border: 0;}
.weui-cellls{background: #fff}
image{vertical-align: middle;}
.header_outer{height: 44px;line-height: 44px;background-color: #fff;color: #333;border-bottom: 1px solid #e9e9e9;display: flex;display: -webkit-flex;display: -webkit-box;}
.header_title{flex: 1;width: 70%;text-align: center;}
.header_k{width: 15%;}
.icon_left:after{content: " ";display: inline-block;height: 13px;width: 13px; border-width: 0 0 2px 2px; border-color: #999;border-style: solid;-webkit-transform: matrix(.71,.71,-.71,.71,0,0);transform: matrix(0.71,.71,-.71,.71,0,0);position: relative;top: 2px;left: 18px;}
/*  */
.container {height: 100%;display: flex;flex-direction: column;align-items: center;justify-content: space-between;padding: 200rpx 0;box-sizing: border-box;} 
.weui-badge{padding: 2rpx 8rpx;min-width: 16rpx;border-radius: 50%;font-size: 24rpx;display:inline-block;text-align: center;color: #fff;line-height: 1.2;background-color: #f43530;vertical-align: middle;}
.weui-cells {background-color: #fff;line-height: 1.47058824;font-size: 17px; overflow: hidden;position: relative;}
.weui-cells:before {top: 0;border-top: 1px solid #e5e5e5;-webkit-transform-origin: 0 0;transform-origin: 0 0; -webkit-transform: scaleY(.5);transform: scaleY(.5);}
.weui-cells:after, .weui-cells:before {content: " ";position: absolute;left: 0;right: 0;height: 1px;color: #e5e5e5;z-index: 2;}
.weui-cell {padding: 20rpx 30rpx;position: relative;display: -webkit-box;display: -webkit-flex;display: flex;-webkit-box-align: center;-webkit-align-items: center;align-items: center;}
.weui-cell:first-child:before{display: none;}
.weui-cell:before { content: " ";position: absolute;top: 0;right: 0;height: 2rpx;border-top: 2rpx solid #e5e5e5;color: #e5e5e5; -webkit-transform-origin: 0 0;transform-origin: 0 0;-webkit-transform: scaleY(.5);transform: scaleY(.5);left: 30rpx;z-index: 2;}
.weui-cell_access {-webkit-tap-highlight-color: rgba(0,0,0,0);color: inherit;}
.weui-cell_access .weui-cell__ft {padding-right: 26rpx; position: relative;}
.weui-cell_access .weui-cell__ft:after {height: 12rpx;width: 12rpx;border-width: 6rpx 6rpx 0 0;margin-top: -8rpx;right: 4rpx;top: 4rpx;}
.weui-cell_access .weui-cell__ft:after {content: " "; display: inline-block;height: 12rpx; width: 12rpx;border-width: 4rpx 4rpx 0 0;border-color: #c8c8cd;border-style: solid;-webkit-transform: matrix(.71,.71,-.71,.71,0,0);transform: matrix(.71,.71,-.71,.71,0,0);top: -4rpx;position: absolute;top: 50%;margin-top: -8rpx;right: 4rpx;}
.weui-cell__bd {-webkit-box-flex: 1;-webkit-flex: 1;flex: 1;}
.weui-cell__ft {text-align: right;color: #999;}
view,text {font-size: 28rpx;}
.weui-flex{display: -webkit-box;display: -webkit-flex;display: flex;}
.weui-flex__item{-webkit-box-flex: 1;-webkit-flex: 1;flex: 1;position: relative;}
.weui-navbar:after {
    content: " ";
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    height: 1px;
    border-bottom: 1px solid #ccc;
    color: #ccc;
    -webkit-transform-origin: 0 100%;
    transform-origin: 0 100%;
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
}
.weui-loading {width: 40rpx;height: 40rpx; display: inline-block;vertical-align: middle;-webkit-animation: e 1s steps(12) infinite;animation: e 1s steps(12) infinite;background: transparent url(data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zd…ByeD0iNSIgcnk9IjUiIHRyYW5zZm9ybT0icm90YXRlKC0zMCAtNS45OCA2NSkiLz48L3N2Zz4=) no-repeat;background-size: 100%;}
.weui-cells__title {margin-bottom: .3em;padding-left: 30rpx; padding-right: 30rpx;color: #999;}
.weui-btn {position: relative;display: block;margin-left: auto;margin-right: auto;padding-left: 14px;padding-right: 14px;box-sizing: border-box;font-size: 18px;text-align: center;text-decoration: none;color: #fff;line-height: 2.55555556;border-radius: 5px;-webkit-tap-highlight-color: rgba(0,0,0,0);overflow: hidden;}
.weui-tab {position: relative;height: 100%;}
.weui-navbar {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    position: relative;
    z-index: 5;
    top: 0;
    width: 100%;
    background-color: #fafafa;
}
.weui-tab__bd { box-sizing: border-box;height: 100%;}
.weui-tab__bd .weui-tab__bd-item {display: none;height: 100%;overflow: auto;}
.weui-tab__bd .weui-tab__bd-item.weui-tab__bd-item--active {display: block;}
.weui-popup__container, .weui-popup__overlay{position: fixed;bottom: 0;left: 0;right: 0;width: 100%;height: 100%;z-index: 102;}
.weui-popup__container{display: none;}
.weui-popup__container.weui-popup__container--visible{display: block;}
.weui-popup__overlay{background-color: rgba(0,0,0,.6);opacity: 0;-webkit-transition: opacity .3s;transition: opacity .3s;}
.weui-popup__container--visible .weui-popup__overlay{opacity: 1;}
.weui-popup__modal{overflow-y: visible;overflow-x: inherit;background: #fff;z-index: 102;display: flex;-webkit-box-orient: vertical;-webkit-box-direction: normal;-webkit-flex-direction: column;flex-direction: column;width: 100%;position: absolute;    bottom: 0;border-radius: 0;    color: #3d4145;-webkit-transition-duration: 0.3s;transition-duration: 0.3s;-webkit-transform: translate3d(0,100%,0);transform: translate3d(0,100%,0); transition-property: opacity,-webkit-transform;}
.weui-popup__modal{background: #fff;top: 20%;overflow: inherit;display: flex;-webkit-box-orient: vertical;-webkit-box-direction: normal;-webkit-flex-direction: column;flex-direction: column;}
.weui-popup__container--visible .weui-popup__modal{opacity: 1;-webkit-transform: translate3d(0,0,0);transform: translate3d(0,0,0);}
.weui-loadmore {width: 65%;margin: 1.5em auto;line-height: 1.6em;font-size: 14px;text-align: center;}
.weui-loadmore_line{border-top: 1px solid #E5E5E5; margin-top: 2.4em;}
.weui-loadmore_line .weui-loadmore__tips{position: relative;top: -0.9em;padding: 0 .55em;background-color: #FFFFFF;color: #808080;}
.weui-loadmore__tips {display: inline-block;vertical-align: middle;}
.white-bg{background-color: #fff;}
/*图标
(1)：左箭头：.icon_left；右箭头：icon_right；上箭头：icon_top；下箭头：icon_bottom；
    (根据需要自行修改大小和颜色可在自己样式文件中修改[class]:after)


*/
.icon_left {position: absolute;width: 30px;height: 30px; top: -5px;left: -5px;}
.icon_left:after{content: "";position: absolute;width: 14px;height: 14px;border: 1px solid #000;border-width: 1px 0 0 1px;-webkit-transform: rotate(315deg);transform: rotate(315deg);/*45上 135右 225下 315左*/top: 8px;left: 7px}
.icon_right {position: absolute;width: 30px; height: 30px;top: -5px;left: -5px;}
.icon_right:after{content: "";position: absolute;width: 14px;height: 14px;border: 1px solid #000; border-width: 1px 0 0 1px;-webkit-transform: rotate(135deg);transform: rotate(135deg);/*45上 135右 225下 315左*/ top: 8px;left: 7px;}
.icon_top {position: absolute;width: 30px;height: 30px;bottom: 2px;right: 10px;}
.icon_top:after{
    content: "";
    position: absolute;
    width: 14px;
    height: 14px;
    border: 1px solid #666;
    border-width: 1px 0 0 1px;-webkit-transform: rotate(45deg);transform: rotate(45deg);/*45上 135右 225下 315左*/top: 8px;left: 7px;}
.icon_bottom {
    position: absolute;
    width: 30px;
    height: 30px;
    bottom: 10px;
    right: 10px;
}
.icon_bottom:after{
    content: "";
    position: absolute;
    width: 14px;
    height: 14px;
    border: 1px solid #666;
    border-width: 1px 0 0 1px;
    -webkit-transform: rotate(225deg);
    transform: rotate(225deg);/*45上 135右 225下 315左*/
    top: 8px;
    left: 7px;
}
.icon_more:after {/*...图标*/
    /*position: absolute;
    right:15px;
    top:14px;*/
    content: "\2022  \2022  \2022 ";
    font-size: 16px;
    color:#000;
}